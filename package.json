{
	"name": "robelest",
	"private": true,
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"dev:convex": "convex dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"sync": "bun run scripts/sync.ts",
		"sync:prod": "bun run --env-file=.env.production scripts/sync.ts",
		"deploy": "vite build && bunx @convex-dev/self-hosting upload --dist ./build --component staticHosting --prod",
		"deploy:dev": "vite build && bunx @convex-dev/self-hosting upload --dist ./build --component staticHosting",
		"deploy:convex": "convex deploy"
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.50.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.1.18",
		"bun-types": "^1.3.9",
		"svelte": "^5.50.2",
		"svelte-check": "^4.3.6",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"vite": "^7.3.1"
	},
	"dependencies": {
		"@convex-dev/self-hosting": "^0.1.1",
		"@lucide/svelte": "^0.563.1",
		"convex": "^1.31.7",
		"convex-svelte": "^0.0.12",
		"katex": "^0.16.28",
		"marked": "^17.0.2",
		"mermaid": "^11.12.2"
	}
}
